{
  "name": "@daijimaps/svgmapviewer",
  "private": "false",
  "version": "0.0.0",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "type": "module",
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev:marche": "vite --config vite.config-marche.ts",
    "dev:host:marche": "vite --config vite.config-marche.ts --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:marche": "tsc -b && vite build --config vite.config-marche.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser",
    "pages:deploy": "pnpm pages:deploy:marche",
    "pages:deploy:marche": "pnpm build:marche && cd dist && wrangler pages deploy ."
  },
  "dependencies": {
    "@xstate/react": "^4.1.3",
    "clsx": "^2.1.1",
    "flatbush": "^4.4.0",
    "fp-ts": "^2.16.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "type-fest": "^4.26.1",
    "vite-plugin-svgr": "^4.2.0",
    "xstate": "^5.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/browser": "^2.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.10.0",
    "playwright": "^1.47.2",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.24.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vitest": "^2.1.2"
  }
}
