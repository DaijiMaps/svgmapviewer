{
  "name": "@daijimaps/svgmapviewer-app",
  "type": "module",
  "private": "false",
  "version": "0.0.0",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": [
    "./dist/index.d.ts"
  ],
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build && tsdown",
    "build:dev": "tsc -b && vite build --mode development --sourcemap true && tsdown",
    "build:prod": "tsc -b && vite build && tsdown",
    "build:watch": "vite build --watch --mode development --sourcemap true",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser"
  },
  "dependencies": {
    "@daijimaps/svgmapviewer": "link:../lib",
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.7.0",
    "clsx": "^2.1.1",
    "flatbush": "^4.4.0",
    "fp-ts": "^2.16.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "type-fest": "^4.41.0",
    "xstate": "^5.20.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@unocss/vite": "^66.2.3",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-react-oxc": "^0.2.3",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "playwright": "^1.53.1",
    "prettier": "3.6.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rolldown": "1.0.0-beta.19",
    "rollup": "^4.44.0",
    "tsdown": "^0.12.8",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "unocss": "^66.2.3",
    "vite": "npm:rolldown-vite@^6.3.21",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4"
  }
}
