{
  "name": "svgmapviewer",
  "version": "0.1.0",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "license": "ISC",
  "author": "Daiji Maps",
  "files": [
    "./dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./box": "./dist/lib/box/prefixed/index.js",
    "./carto": "./dist/lib/carto/index.js",
    "./carto-objects": "./dist/lib/carto/objects/index.js",
    "./carto-symbols": "./dist/lib/carto/symbols/index.js",
    "./geo": "./dist/lib/geo/index.js",
    "./map": "./dist/lib/map/index.js",
    "./map-floors": "./dist/lib/map/floors/index.js",
    "./matrix": "./dist/lib/matrix/prefixed/index.js",
    "./search": "./dist/lib/search/index.js",
    "./tuple": "./dist/lib/tuple/index.js",
    "./vec": "./dist/lib/vec/prefixed/index.js"
  },
  "scripts": {
    "build": "tsgo -b && rslib build",
    "lint": "eslint . && oxlint --type-aware --type-check",
    "publish:jsr": "pnpm dlx jsr publish",
    "test": "rstest",
    "test:dev": "rstest --watch"
  },
  "dependencies": {
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.14.1",
    "clsx": "^2.1.1",
    "flatbush": "^4.5.0",
    "fp-ts": "^2.16.11",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "type-fest": "^5.4.1",
    "xstate": "^5.25.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.4.3",
    "@rslib/core": "^0.19.2",
    "@rstest/core": "^0.7.9",
    "@testing-library/react": "^16.3.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "24.x",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260115.1",
    "happy-dom": "^20.3.0",
    "playwright": "^1.57.0",
    "prettier": "^3.8.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*": "prettier --write --cache --ignore-unknown"
  }
}
