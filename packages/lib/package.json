{
  "name": "svgmapviewer",
  "type": "module",
  "version": "0.0.2-alpha.1",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./box": "./dist/box.js",
    "./carto": "./dist/carto.js",
    "./carto-objects": "./dist/carto-objects.js",
    "./carto-symbols": "./dist/carto-symbols.js",
    "./geo": "./dist/geo.js",
    "./map": "./dist/map.js",
    "./matrix": "./dist/matrix.js",
    "./search": "./dist/search.js",
    "./tuple": "./dist/tuple.js",
    "./vec": "./dist/vec.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && tsdown",
    "build:dev": "tsc -b && tsdown --sourcemap",
    "build:prod": "tsc -b && tsdown",
    "build:watch": "tsdown --watch --sourcemap",
    "lint": "eslint .",
    "preview": "vite preview",
    "publish:jsr": "pnpm dlx jsr publish",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser"
  },
  "dependencies": {
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.8.3",
    "clsx": "^2.1.1",
    "flatbush": "^4.4.1",
    "fp-ts": "^2.16.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "type-fest": "^4.41.0",
    "xstate": "^5.20.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.33",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@unocss/vite": "^66.3.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-oxc": "^0.3.0",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "oxlint": "^1.8.0",
    "playwright": "^1.54.1",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rolldown": "1.0.0-beta.29",
    "tsdown": "^0.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "unocss": "^66.3.3",
    "vite": "npm:rolldown-vite@^7.0.11",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4"
  }
}
