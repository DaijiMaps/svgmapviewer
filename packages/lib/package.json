{
  "name": "@daijimaps/svgmapviewer",
  "type": "module",
  "private": "false",
  "version": "0.0.0",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./box": "./dist/box.js",
    "./carto": "./dist/carto.js",
    "./carto-objects": "./dist/carto-objects.js",
    "./carto-symbols": "./dist/carto-symbols.js",
    "./geo": "./dist/geo.js",
    "./matrix": "./dist/matrix.js",
    "./search": "./dist/search.js",
    "./tuple": "./dist/tuple.js",
    "./vec": "./dist/vec.js"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:watch": "vite build --watch --mode development --sourcemap true",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser"
  },
  "dependencies": {
    "@xstate/react": "^4.1.3",
    "clsx": "^2.1.1",
    "flatbush": "^4.4.0",
    "fp-ts": "^2.16.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "type-fest": "^4.26.1",
    "vite-plugin-svgr": "^4.2.0",
    "xstate": "^5.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/browser": "^2.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "playwright": "^1.48.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.24.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vitest": "^2.1.2"
  }
