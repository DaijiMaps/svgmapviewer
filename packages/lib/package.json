{
  "name": "svgmapviewer",
  "type": "module",
  "version": "0.0.2-alpha.8",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./box": "./dist/lib/box/prefixed/index.js",
    "./carto": "./dist/lib/carto/index.js",
    "./carto-objects": "./dist/lib/carto/objects/index.js",
    "./carto-symbols": "./dist/lib/carto/symbols/index.js",
    "./geo": "./dist/lib/geo/index.js",
    "./map": "./dist/lib/map/index.js",
    "./matrix": "./dist/lib/matrix/prefixed/index.js",
    "./search": "./dist/lib/search/index.js",
    "./search-worker": "./dist/search-worker.js",
    "./tuple": "./dist/lib/tuple/index.js",
    "./vec": "./dist/lib/vec/prefixed/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && rslib build",
    "lint": "eslint .",
    "preview": "vite preview",
    "publish:jsr": "pnpm dlx jsr publish",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser"
  },
  "dependencies": {
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.8.5",
    "clsx": "^2.1.1",
    "flatbush": "^4.4.1",
    "fp-ts": "^2.16.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "type-fest": "^4.41.0",
    "xstate": "^5.20.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rslib/core": "^0.11.2",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.33",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@unocss/vite": "^66.4.2",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-react-oxc": "^0.4.0",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "oxlint": "^1.11.1",
    "playwright": "^1.54.2",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rolldown": "1.0.0-beta.31",
    "tsdown": "^0.13.4",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "unocss": "^66.4.2",
    "vite": "npm:rolldown-vite@^7.1.0",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node",
        "typescript"
      ]
    }
  }
}
