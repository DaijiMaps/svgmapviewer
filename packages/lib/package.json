{
  "type": "module",
  "name": "svgmapviewer",
  "version": "0.0.2-alpha.13",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./box": "./dist/lib/box/prefixed/index.js",
    "./carto": "./dist/lib/carto/index.js",
    "./carto-objects": "./dist/lib/carto/objects/index.js",
    "./carto-symbols": "./dist/lib/carto/symbols/index.js",
    "./geo": "./dist/lib/geo/index.js",
    "./map": "./dist/lib/map/index.js",
    "./map-floors": "./dist/lib/map/floors/index.js",
    "./matrix": "./dist/lib/matrix/prefixed/index.js",
    "./search": "./dist/lib/search/index.js",
    "./search-worker": "./dist/search-worker.js",
    "./tuple": "./dist/lib/tuple/index.js",
    "./vec": "./dist/lib/vec/prefixed/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && rslib build",
    "lint": "eslint . && oxlint",
    "preview": "vite preview",
    "publish:jsr": "pnpm dlx jsr publish",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser"
  },
  "dependencies": {
    "@xstate/react": "^6.0.0",
    "@xstate/store": "^3.9.3",
    "clsx": "^2.1.1",
    "flatbush": "^4.5.0",
    "fp-ts": "^2.16.11",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "type-fest": "^5.0.1",
    "xstate": "^5.22.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.15.0",
    "@rslint/core": "^0.1.13",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@unocss/vite": "^66.5.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-react-oxc": "^0.4.2",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "oxlint": "^1.20.0",
    "playwright": "^1.56.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rolldown": "1.0.0-beta.42",
    "tsdown": "^0.15.6",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.0",
    "unocss": "^66.5.2",
    "vite": "npm:rolldown-vite@^7.1.16",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*": "prettier --write --cache --ignore-unknown"
  }
}
