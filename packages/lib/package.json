{
  "name": "@daijimaps/svgmapviewer",
  "type": "module",
  "private": "false",
  "version": "0.0.0",
  "keywords": [
    "SVG",
    "map",
    "viewer"
  ],
  "author": "Daiji Maps",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./box": "./dist/box.js",
    "./carto": "./dist/carto.js",
    "./carto-objects": "./dist/carto-objects.js",
    "./carto-symbols": "./dist/carto-symbols.js",
    "./geo": "./dist/geo.js",
    "./matrix": "./dist/matrix.js",
    "./search": "./dist/search.js",
    "./tuple": "./dist/tuple.js",
    "./vec": "./dist/vec.js"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development --sourcemap true",
    "build:prod": "tsc -b && vite build",
    "build:watch": "vite build --watch --mode development --sourcemap true",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:unit": "vitest --workspace=vitest.workspace.ts --project unit",
    "test:jsdom": "vitest --workspace=vitest.workspace.ts --project jsdom",
    "test:browser": "vitest --workspace=vitest.workspace.ts --project browser"
  },
  "dependencies": {
    "@xstate/react": "^5.0.4",
    "@xstate/store": "^3.6.2",
    "clsx": "^2.1.1",
    "flatbush": "^4.4.0",
    "fp-ts": "^2.16.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "type-fest": "^4.41.0",
    "vite-plugin-svgr": "^4.3.0",
    "xstate": "^5.19.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@unocss/vite": "^66.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "playwright": "^1.52.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.41.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "unocss": "^66.1.2",
    "vite": "^6.3.5",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitest": "^3.1.4"
  }
}
