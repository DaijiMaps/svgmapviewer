    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-libcss": "^1.1.2",
    "vitest": "^4.0.16"
