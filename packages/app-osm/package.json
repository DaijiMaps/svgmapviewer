    "react-dom": "^19.2.3",
    "svgmapviewer": "workspace:*",
    "type-fest": "^5.3.1",
    "xstate": "^5.25.0"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.4.2",
    "@rslib/core": "^0.18.4",
